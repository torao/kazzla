ALIAS=volumenode
FILENAME=kazzla
PASSWORD=kazzla
#STORETYPE=pkcs12
STORETYPE=JKS
#STOREEXT=p12
STOREEXT=jks
keytool -genkeypair -alias $ALIAS -keyalg RSA -keysize 1024 -keypass $PASSWORD -validity 730 -storetype $STORETYPE -keystore $FILENAME.$STOREEXT -storepass $PASSWORD -dname "cn=volume.kazzla.com,ou=development,o=kazzla,c=COM"
keytool -certreq -alias $ALIAS -file $FILENAME.csr -keystore $FILENAME.$STOREEXT -storetype $STORETYPE -storepass $PASSWORD -keypass $PASSWORD
keytool -exportcert -alias $ALIAS -file $FILENAME.crt -storetype $STORETYPE -keystore $FILENAME.$STOREEXT -storepass $PASSWORD 
keytool -list -alias $ALIAS -storetype $STORETYPE -keystore $FILENAME.$STOREEXT -storepass $PASSWORD -v

